{
  "name": "dhl/module-rates-express",
  "version": "0.1.0",
  "type": "magento2-module",
  "description": "Rates in Checkout implementation for DHL Express API",
  "license": "OSL-3.0",
  "authors": [
    {
      "email": "max.melzer@netresearch.de",
      "name": "Max Melzer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Dhl\\ExpressRates\\": ""
    },
    "files": [
      "registration.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Dhl\\ExpressRates\\": [
        "Test/Integration"
      ]
    }
  },
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "require": {
    "ext-intl": "*",
    "psr/log": "~1.0",
    "magento/module-shipping": "^100.1.0",
    "dhl/sdk-api-express": "@dev"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "*"
  }
}
